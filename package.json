{
  "name": "@metolabs/mx-bank-transaction-parser",
  "version": "0.1.0",
  "description": "JavaScript library for parsing transaction files from Mexican banks (TXT, CSV).",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "bank",
    "transaction",
    "parser",
    "mexico",
    "csv",
    "txt",
    "scotiabank",
    "bbva",
    "hsbc",
    "banorte",
    "santander",
    "banregio",
    "afirme",
    "banbajio"
  ],
  "author": "Gerardo Ramirez <gera.96@icloud.com>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "csv-parse": "^6.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "rollup": "^4.45.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.esm.js"
    }
  }
}